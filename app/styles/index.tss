"#MainWindow": {
	backgroundColor: "#EEE"
},
"#MainWindow[platform=android]": {
	navBarHidden: true,
	exitOnClose: true
},
"#MainWindow[platform=android formFactor=tablet]": {
	orientationModes: [
		Titanium.UI.LANDSCAPE_LEFT
	]
},
"#MainWindow[platform=android formFactor=handheld]": {
	orientationModes: [
		Titanium.UI.PORTRAIT
	]
},
"#ContentWrapper": {
	top: "0dp",
	bottom: "60dp"
}