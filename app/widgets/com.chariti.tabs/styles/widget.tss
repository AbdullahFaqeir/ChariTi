"#Wrapper": {
	height: "60dp",
	bottom: "0dp"
},
"#TabContainer": {
	layout: "horizontal",
	zIndex: 100,
	height: "60dp",
	width: Ti.UI.FILL,
	backgroundImage: "/com.chariti.tabs/overlay.png"
},
"#indicator": {
	zIndex: 99,
	top: "1dp",
	backgroundGradient: {
		type: "linear",
		colors: [
			"#000",
			"transparent"
		],
		startPoint: {
			x: 0,
			y: 0
		},
		endPoint: {
			x: 0,
			y: "100%"
		},
		backFillStart: true
	},
	opacity: 0.4
}